{
  "version": 2,
  "crons": [
    {
      "path": "/api/cron-price-scanner",
      "schedule": "*/10 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/brainrots/(.*)",
      "dest": "/public/brainrots/$1"
    },
    {
      "src": "/brainrots-mapping.json",
      "dest": "/public/brainrots-mapping.json"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/public/$1"
    }
  ]
}
