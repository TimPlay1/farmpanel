{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/brainrots/(.*)",
      "dest": "/public/brainrots/$1"
    },
    {
      "src": "/brainrots-mapping.json",
      "dest": "/public/brainrots-mapping.json"
    },
    {
      "src": "/((?!api/).*)",
      "dest": "/public/$1"
    }
  ]
}
